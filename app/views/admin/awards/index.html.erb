<%= render 'shared/adminnav' %>
<div class="col-md-9">
<h4> List Of Awards</h4><%= link_to "+ New", new_admin_award_path, class: "btn btn-primary"  %>
<table>
  <tr>
  <th class="id">ID</th>
  <th class="title">Title</th>
  <th class="body">Body</th>
  <th class="created"> Created at</th>
  <th class="updated">Updated at</th>
  <th class="image">Image</th>
  <th class="edit"></th>
  </tr>
<% @awards.each do |award| %>
  <tr>
  <td class="id"><%= award.id %></td>
  <td class="title">  <%= link_to award.title, admin_award_path(award) %></td>
  <td class="body"><%= award.body %></td>
  <td class="created"> <%= award.created_at.strftime('%d-%m-%Y') %></td>
  <td class="updated"><%= award.updated_at.strftime('%d-%m-%Y') %></td>
  <td class="image"> <%= image_tag award.image.artistmain.url, :class =>  "imagethumb" %></td>
  <td class="edit"> <%= link_to "Edit", edit_admin_award_path(award), :class =>  "btn btn-primary", :method => :get  %> 
   <%= link_to 'Delete', admin_award_path(award), method: :delete, class: "btn btn-primary"  %></td>

  </tr>

<% end %>
</table>
</div></div>




