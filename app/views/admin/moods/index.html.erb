<%= render 'shared/adminnav' %>
<div class="col-md-9">
<h4> List Of Mood Tags</h4><%= link_to "+ New", new_admin_mood_path, class: "btn btn-primary"  %>
<br /><br />
<% @moods.each do |mood| %>
   <div class="col-md-2" style="padding-left:0px; padding-top:10px;">
   <%= link_to mood.name, admin_mood_path(mood) %> <br />
   <%= link_to 'Delete', admin_mood_path(mood), method: :delete, class: "btn btn-primary"  %>
   </div> 
<% end %>
 
</div>
