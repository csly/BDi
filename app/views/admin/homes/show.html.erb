<%= render 'shared/adminnav' %> 
<div class="col-md-9">
<h4> Dashboard Quick Links</h4>
 <br />

<div class="related">Latest News Posts (showing latest 3 of <%= @articles.count %>)</div> <br />
<table width="100%" class="admin-table">
<tr>
<td class="one">ID</td>
<td class="two">Title</td>
<td class="three">Body</td>
<td class="four">Links</td>
<% @articles.each do |article| %>
<tr>
<td class="one"><p><%= article.id %></p></td>
<td class="two"><p><%= link_to article.title, edit_admin_article_path(article)  %></p></td>
<td class="three"><%= article.artistpreview.html_safe %></td>
<td class="four"><%= link_to "Edit", edit_admin_article_path(article), :class =>  "btn btn-primary", :method => :get  %> 
   <%= link_to 'Delete', admin_article_path(article), method: :delete, class: "btn btn-primary"  %> </td>
   </tr>
<% end %>

</table>
 <br /> <br />
