<%= simple_form_for([:admin, @release], html: {multipart: true}) do |f| %>
  <%= f.input :title %>
  <%= f.input :body %>  
  <%= f.input :date %>
  <%= f.input :links %>
  <%= f.input :amazon %>
  <%= f.input :itunes %>
  <%= f.association :artists, collection: Artist.order('name ASC') %>
  <%= f.association :genres, collection: Genre.order('name ASC') %>
  <%= f.association :productions, collection: Production.order('name ASC') %>
  <%= f.file_field :image %> <br />
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
<br /><br />