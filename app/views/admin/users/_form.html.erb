<%= simple_form_for([:admin, @user], html: {multipart: true}) do |f| %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :client %> 
  <%= f.input :admin %> 
  <%= f.input :company, label: 'Production Company Name'  %> 
  <%= f.submit class: 'btn btn-primary' %>
<% end %>