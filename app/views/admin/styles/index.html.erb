<%= render 'shared/adminnav' %>
<div class="col-md-9">
<h4> List Of Style Tags</h4><%= link_to "+ New", new_admin_style_path, class: "btn btn-primary"  %>
<br /><br />
<% @styles.each do |style| %>
   <div class="col-md-2" style="padding-left:0px; padding-top:10px;">
   <%= link_to style.name, admin_style_path(style) %> <br />
   <%= link_to 'Delete', admin_style_path(style), method: :delete, class: "btn btn-primary"  %>
   </div> 
<% end %>
 
</div>
<%= render 'shared/adminnavfooter' %>